name: dockercompose15881005520393348121
services:
  osservices.apiservice:
    build:
      context: C:\Users\KamillaHÃ©ricadeSousa\Downloads\ControleOrdemDeServicoJR\ControleOrdemDeServico\ControleOrdemDeServico.ApiService
      dockerfile: Dockerfile
    container_name: osservices-api
    depends_on:
      rabbitmq:
        condition: service_healthy
        required: true
      sqlserver:
        condition: service_healthy
        required: true
    environment:
      ASPNETCORE_ENVIRONMENT: Development
      ASPNETCORE_HTTP_PORTS: "5021"
      ConnectionStrings__CreateTable: Server=sqlserver,1433;Database=master;User Id=sa;Password=SqlServer2024!Strong#;TrustServerCertificate=True;Encrypt=False;
      ConnectionStrings__DefaultConnection: Server=sqlserver,1433;Database=OsServiceDb;User Id=sa;Password=SqlServer2024!Strong#;TrustServerCertificate=True;Encrypt=False;
    networks:
      osservices: null
    ports:
      - mode: ingress
        target: 8080
        published: "5000"
        protocol: tcp
      - mode: ingress
        target: 5021
        published: "5021"
        protocol: tcp
  rabbitmq:
    container_name: rabbitmq
    environment:
      RABBITMQ_DEFAULT_PASS: guest
      RABBITMQ_DEFAULT_USER: guest
    healthcheck:
      test:
        - CMD
        - rabbitmqctl
        - status
      timeout: 5s
      interval: 10s
      retries: 5
    image: rabbitmq:4.1.5-management-alpine
    networks:
      osservices: null
    ports:
      - mode: ingress
        target: 15672
        published: "15672"
        protocol: tcp
      - mode: ingress
        target: 5672
        published: "5672"
        protocol: tcp
    volumes:
      - type: volume
        source: mq_data
        target: /var/lib/rabbitmq/mnesia
        volume: {}
  sqlserver:
    container_name: sqlserver
    environment:
      ACCEPT_EULA: "Y"
      MSSQL_PID: Developer
      MSSQL_SA_PASSWORD: SqlServer2024!Strong#
    healthcheck:
      test:
        - CMD-SHELL
        - timeout 1 bash -c '</dev/tcp/localhost/1433' || exit 1
      timeout: 5s
      interval: 10s
      retries: 20
      start_period: 30s
    image: mcr.microsoft.com/mssql/server:2022-latest
    networks:
      osservices: null
    ports:
      - mode: ingress
        target: 1433
        published: "1433"
        protocol: tcp
    volumes:
      - type: volume
        source: sqlserver_data
        target: /var/opt/mssql
        volume: {}
networks:
  osservices:
    name: dockercompose15881005520393348121_osservices
    driver: bridge
volumes:
  mq_data:
    name: dockercompose15881005520393348121_mq_data
  sqlserver_data:
    name: dockercompose15881005520393348121_sqlserver_data